{
    "name": "chassisphp/chassisphp-skeleton",
    "description": "Skeleton app for the ChassisPHP Framework. This app is a starting point for developing your own site/application.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Roger Creasy",
            "email": "roger.creasy@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Database\\": "Database"
        }
    },
    "require": {
        "chassisphp/chassisphp": "^0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/process": "^4.1"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
